{
  "name": "encodeuri-plus",
  "version": "0.1.0",
  "license": "MIT",
  "files": [
    "lib",
    "src",
    "tsconfig.json"
  ],
  "description": "encodeURI that is safe, and doesn't do too much in a specific scenario",
  "main": "lib/index.js",
  "unpkg": "lib/index.min.js",
  "types": "lib/index.d.ts",
  "author": {
    "name": "Pacharapol Withayasakpunt",
    "email": "patarapolw@gmail.com",
    "url": "https://www.polv.cc"
  },
  "repository": "github:patarapolw/encodeuri-plus",
  "keywords": [
    "encodeuri",
    "encodeuricomponent",
    "querystring"
  ],
  "scripts": {
    "engine-strict": "echo 'engine-strict=true' > .npmrc",
    "build": "tsc -p src/tsconfig.json",
    "browserify": "webpack && cp lib/index.umd.js docs/index.umd.js",
    "prepack": "yarn build && yarn browserify"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "eslint": ">=6.2.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": ">=2.18.0",
    "eslint-plugin-node": ">=9.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": ">=4.2.1",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-standard": ">=4.0.0",
    "prettier": "^2.0.5",
    "ts-loader": "^8.0.0",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "engines": {
    "node": "12.x",
    "npm": "please-use-yarn",
    "yarn": "1.x"
  }
